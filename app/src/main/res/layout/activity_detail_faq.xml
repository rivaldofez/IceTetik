<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primaryBackgroundColor"
    tools:context=".page.settings.faq.DetailFaqActivity">

    <TextView
        style="@style/ItemButtonFaq"
        android:id="@+id/tv_title_faq"
        android:layout_width="match_parent"
        android:textAlignment="center"
        android:layout_margin="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="Apa itu ice-tetik"
        />

    <TextView
        android:id="@+id/tv_answer_faq"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:fontFamily="@font/league_spartan_medium"
        android:layout_margin="16dp"
        android:textAlignment="viewStart"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_title_faq"
        tools:text="Lorem ipsum dolor sit amet"
        />


</androidx.constraintlayout.widget.ConstraintLayout>